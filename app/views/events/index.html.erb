<% content_for :header do %>
  <%= javascript_include_tag 'http://www.google.com/jsapi' %>
  <%= javascript_include_tag :timeline %>
  <%= stylesheet_link_tag :timeline %>
  <%= timeline(@events, 'timeline',
        width: '100%',
        height: '450px',
        min: @from,
        max: @till,
        style: :dot,
        zoomMin: 1000 * 60 * 60,
        showNavigation: true,
      ) %>
<% end %>

<% content_for :navbar_right do %>
  <%= render partial: 'form', locals: {host_filter: @host_filter, priority: @priority, from: @from, till: @till} %>
<% end %>

<div class="row">
  <div class="col-lg-12">
    <div id="timeline"></div>
  </div>
</div>
<div class="row">
  <div class="col-lg-12">
    <%= render partial: 'paginator', locals: {host_filter: @host_filter, priority: @priority, from: @from, till: @till} %>
  </div>
</div>
