<% content_for :header do %>
  <%= javascript_include_tag 'http://www.google.com/jsapi' %>
  <%= javascript_include_tag :timeline %>
  <%= stylesheet_link_tag :timeline %>
  <%= timeline(@events, 'timeline',
        width: '100%',
        height: '300px',
        min: @from,
        max: @till,
        style: :dot
      ) %>
<% end %>

<h1>Events#index</h1>
<p>Find me in app/views/events/index.html.erb</p>

<div id="timeline"></div>
